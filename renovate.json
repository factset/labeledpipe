{
  "automerge": true,
  "docker": {
    "digest": {
      "groupName": "Docker Digests",
      "group": {
        "prTitle": "Update and group Docker digests"
      },
      "automerge": true
    }
  },
  "labels": ["refactor"],
  "lockFileMaintenance": {
    "enabled": true
  },
  "major": {
    "automerge": false
  },
  "node": {
    "enabled": true,
    "supportPolicy": ["lts", "current"]
  },
  "pinVersions": false,
  "prCreation": "not-pending",
  "rebaseStalePrs": true,
  "unpublishSafe": true
}
